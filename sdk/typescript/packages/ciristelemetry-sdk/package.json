{
  "name": "@cirisai/ciristelemetry-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for CIRIS Telemetry System",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/ciristelemetry-sdk.min.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc --module commonjs --outDir dist",
    "build:esm": "tsc --module esnext --outDir dist --declaration false && mv dist/index.js dist/index.esm.js",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "build:browser": "webpack --config webpack.config.js",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "ciris",
    "telemetry",
    "monitoring",
    "metrics",
    "analytics",
    "typescript",
    "sdk"
  ],
  "author": "CIRIS AI",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CIRISAI/CIRISManager.git",
    "directory": "sdk/typescript/packages/ciristelemetry-sdk"
  },
  "bugs": {
    "url": "https://github.com/CIRISAI/CIRISManager/issues"
  },
  "homepage": "https://github.com/CIRISAI/CIRISManager#readme"
}